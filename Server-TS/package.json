{
  "name": "fusion360-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for Autodesk Fusion 360",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:stdio": "node dist/index.js --server_type=stdio",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests",
    "validate-manifest": "mcpb validate manifest.json",
    "package": "npm run build && npm run validate-manifest && mcpb pack . fusion360-mcp.mcpb"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "posthog-node": "^4.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@anthropic-ai/mcpb": "^2.1.2",
    "@types/archiver": "^6.0.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "archiver": "^7.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mcp",
    "fusion360",
    "autodesk",
    "cad",
    "3d-modeling"
  ],
  "license": "MIT"
}
